<script>

function initMap() {
        var uluru = {lat: 55.4872883, lng: 9.4713059};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 17,
          center: uluru,
            disableDefaultUI: true
        });
       
        var marker = new google.maps.Marker({
          position: uluru,
            icon: 'http://svgshare.com/i/zF.svg',
          map: map,
            anchor: new google.maps.Point(0, 0)
        });
      }
</script>
 <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCohs4SZmUwnlLvX4MdQ0peSIV40W8Jl-k&callback=initMap">
